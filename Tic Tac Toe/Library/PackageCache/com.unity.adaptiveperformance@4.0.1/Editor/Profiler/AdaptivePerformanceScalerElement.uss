#ap-scaler-element {
    flex-grow: 1;
    width: 40px;
    align-items: center;
}

#ap-scaler-element-content {
    flex-grow: 1;
    width: 20px;
    flex-direction: column;
    align-items: center;
}

#ap-scaler-element-label {
    -unity-font-style: bold;
}

#ap-scaler-element-max-label {
    -unity-font-style: bold;
}

#ap-scaler-element-bar {
    background-color: rgba(77, 77, 77, 0.5);
    height: 200px;
    width: 20px;
}

#ap-scaler-element-bar-fill {
}

#ap-scaler-element-level-label {
    -unity-text-align: middle-center;
}
